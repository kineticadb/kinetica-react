{
    "kUser": "admin",
    "kPass": "",
    "kUrl": "http://localhost:9191",
    "wmsLayer": {
        "label": "NYC Taxi",
        "id": "nyc_taxi_id",
        "kineticaSettings": {
            "renderType": "heatmap",
            "view": "demo.nyctaxi",
            "baseTable": "demo.nyctaxi",
            "blurRadius": 5,
            "colormap": "magma",
            "wkt": "",
            "longitude": "pickup_longitude",
            "latitude": "pickup_latitude"
        },
        "popupTemplate": "<div style=\"font-size:22px;margin-top: 35px;\"><label style=\"font-weight:bold\">Pickup Location:</label> {{pickup_longitude}},{{pickup_latitude}}<br/><label style=\"font-weight:bold\">Number of passengers:</label> {{passenger_count}}<br/><label style=\"font-weight:bold\">Vendor:</label> {{vendor_id}}<br/><label style=\"font-weight:bold\">Fare Amount:</label> {{fare_amount}}<br/><label style=\"font-weight:bold\">Payment Type:</label> {{payment_type}}<br/><label style=\"font-weight:bold\">Distance:</label> {{trip_distance}}</div>",
        "enablePopup": true,
        "visible": true,
        "opacity": 80,
        "minZoom": 0,
        "maxZoom": 24
    },
    "vectorLayer": {
        "label": "US States",
        "id": "us_states_id",
        "kineticaSettings": {
            "vectorLayerType": "vectorLayerType",
            "view": "ki_home.us_states_buff",
            "baseTable": "ki_home.us_states",
            "attributes": ["WKT", "NAME", "GEOID"]
        },
        "style": {
            "stroke-width": 0.6,
            "stroke-color": "#E69A8D",
            "fill-color": "#5F4B8B"
        },
        "popupTemplate": "<div style=\"font-size:22px;margin-top: 35px;\"><label style=\"font-weight:bold\">Name:</label> {{NAME}},<br/><label style=\"font-weight:bold\">GEOID:</label> {{GEOID}}</div>",
        "enablePopup": true,
        "visible": true,
        "opacity": 90,
        "minZoom": 0,
        "maxZoom": 18
    }
}